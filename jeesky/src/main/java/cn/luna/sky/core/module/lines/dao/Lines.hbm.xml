<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">	
<hibernate-mapping>
		<class name="cn.luna.sky.core.module.lines.entity.TrainLines" 
	 table="train_lines"	dynamic-update="true" dynamic-insert="true"
	  optimistic-lock="version">
        
		<id name="id">
			<generator class="assigned"/>
	    </id>        
		<version name="lockVersion" />
		<property name="trainNum"/>
		<property name="trainStartTime"/>
		<property name="trainEndTime"/>
		<property name="trainStartStation"/>
		<property name="trainEndStation"/>
		<property name="ticketsPrice"/>
		<property name="ticketsLeft"/>
		
    </class>
</hibernate-mapping>